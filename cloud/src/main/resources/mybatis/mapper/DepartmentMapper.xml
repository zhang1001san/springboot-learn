<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhang.cloud.mapper.DepartmentMapper">

    <select id="queryDepartment" resultType="com.zhang.cloud.bean.Department">
        select *
        from `department`
        where id = #{id}
    </select>
</mapper>